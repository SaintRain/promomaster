{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field %}
    {% if object.group.caption  is defined %}
        <div class="label label-default">{{ object.group.caption }}</div>
    {% endif %}
    <div style="width:250px"><a href="{{ path('admin_core_config_config_edit',{id:object.id}) }}">{{ object.caption }}</a></div>
{% endblock %}
