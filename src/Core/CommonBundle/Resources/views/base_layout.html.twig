            <!--End of Zopim Live Chat Script-->

            <script>
                $zopim(function () {
                    {% if app.user.firstName is defined %}
                    $zopim.livechat.setName("{{ app.user.firstname }}");
                    $zopim.livechat.setEmail("{{ app.user.email }}");
                    {% endif %}
                    $zopim.livechat.window.hide();
                });

                $(function () {
                    $(document).on('click', '.onlinechat', function () {
                        $zopim.livechat.window.show();
                    })
                })
            </script>



